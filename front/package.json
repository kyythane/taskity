{
  "name": "taskity",
  "description": "A task tracking application",
  "version": "1.0.0",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyythane/taskity.git"
  },
  "scripts": {
    "build:app": "routify -b && rollup -c",
    "build:gql": "rm -rf src/generated && cd ../back/app && mix export.schema && cd ../../front && apollo client:codegen --target typescript --mergeInFieldsFromFragmentSpreads --useReadOnlyTypes --tsFileExtension d.ts --outputFlat src/generated",
    "check": "npm run check:svelte && npm run check:type && npm run check:lint",
    "check:svelte": "svelte-check",
    "check:type": "tsc -p ./tsconfig.json",
    "check:lint": "eslint",
    "fix:lint": "esling --fix",
    "dev": "run-p routify rollup",
    "rollup": "rollup -cw",
    "routify": "routify",
    "start": "sirv public --single"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@sveltech/routify": "^1.9.7",
    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss-font-family-system-ui": "^4.3.0",
    "postcss-load-config": "^2.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.26.3",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^6.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.17",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^1.6.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@absinthe/socket": "^0.2.1",
    "@absinthe/socket-apollo-link": "^0.2.1",
    "@types/absinthe__socket": "^0.2.0",
    "@types/absinthe__socket-apollo-link": "^0.2.0",
    "@types/js-cookie": "^2.2.6",
    "@types/phoenix": "^1.5.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "graphql": "^14.7.0",
    "js-cookie": "^2.2.1",
    "phoenix": "^1.5.4",
    "sirv-cli": "^1.0.0",
    "svelte-apollo": "^0.3.0"
  }
}