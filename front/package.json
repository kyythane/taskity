{
  "name": "taskity",
  "description": "A taks tracking application",
  "version": "1.0.0",
  "license": "Unlicense",
  "scripts": {
    "build:app": "rollup -c",
    "build:gql": "rm -rf src/generated && cd ../back/app && mix absinthe.schema.sdl --schema TaskityWeb.Schema ../../front/gql/schema.graphql && cd ../../front && apollo client:codegen --target typescript --mergeInFieldsFromFragmentSpreads --useReadOnlyTypes --outputFlat src/generated",
    "check:all": "npm run check:svelte && npm run check:type && npm run check:lint",
    "check:svelte": "svelte-check",
    "check:type": "tsc -p ./tsconfig.json",
    "check:lint": "eslint --fix",
    "dev": "rollup -c -w",
    "start": "sirv public --single"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-svelte3": "^2.7.3",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^6.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@absinthe/socket": "^0.2.1",
    "@absinthe/socket-apollo-link": "^0.2.1",
    "@types/absinthe__socket": "^0.2.0",
    "@types/absinthe__socket-apollo-link": "^0.2.0",
    "@types/js-cookie": "^2.2.6",
    "@types/phoenix": "^1.5.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "graphql": "^15.3.0",
    "js-cookie": "^2.2.1",
    "phoenix": "^1.5.4",
    "sirv-cli": "^1.0.0",
    "svelte-apollo": "^0.3.0"
  }
}